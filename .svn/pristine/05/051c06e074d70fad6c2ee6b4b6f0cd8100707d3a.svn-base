package com.zte.pojo;

public class City {
	private int id;
	private String name;
	private String key;
	public City() {
		super();
		// TODO Auto-generated constructor stub
	}
	public City(String key) {
		super();
		this.key = key;
	}
	public City(String name, String key) {
		super();
		this.name = name;
		this.key = key;
	}
	public City(int id, String name, String key) {
		super();
		this.id = id;
		this.name = name;
		this.key = key;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getKey() {
		return key;
	}
	public void setKey(String key) {
		this.key = key;
	}
	
}
